<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- REVERT TO STOCK -->
    <!-- QUICKSETTINGS -->
    <style name="systemui_theme" parent="@android:style/Theme.DeviceDefault">
        <item name="android:colorPrimary">@android:color/darker_gray</item>
        <item name="android:colorControlActivated">@android:color/darker_gray</item>
    </style>
    <style name="TextAppearance" />
    <style name="TextAppearance.QS">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <!-- QuickSettings Toolbar text -->
    <style name="TextAppearance.QS.DetailHeader">
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <!-- Volume Panel Do not disturb title -->
    <style name="TextAppearance.Volume.ZenSummary" parent="@style/TextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <!-- Volume Panel Do not disturb description -->
    <style name="TextAppearance.Volume.ZenDetail" parent="@style/TextAppearance">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColor">#ffb0b3c5</item>
    </style>

</resources>
