<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.ThemeOverlay.AppCompat" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorControlNormal">@android:color/holo_blue_dark</item>
        <item name="android:colorControlActivated">@android:color/holo_orange_dark</item>
        <item name="android:colorButtonNormal">?android:colorButtonNormal</item>
        <item name="android:colorControlHighlight">#ffffffff</item>
        <item name="android:colorPrimary">@android:color/holo_green_dark</item>
        <item name="android:colorPrimaryDark">@android:color/holo_green_light</item>
        <item name="android:colorAccent">@android:color/holo_orange_dark</item>
    </style>
    <style name="Platform.ThemeOverlay.AppCompat.Dark" parent="@android:style/Theme.Material.Light.DarkActionBar" />
    <style name="Base.ThemeOverlay.AppCompat.Dark" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorForeground">@android:color/holo_blue_dark</item>
        <item name="android:colorBackground">#ffffffff</item>
        <item name="android:textColorPrimary">#ff1e1e1e</item>
        <item name="android:colorBackgroundCacheHint">#95ffffff</item>
        <item name="android:colorControlNormal">@android:color/holo_blue_dark</item>
        <item name="android:colorControlHighlight">@android:color/holo_orange_dark</item>
        <item name="isLightTheme">true</item>
    </style>
    <style name="Base.ThemeOverlay.AppCompat.Dark.ActionBar" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorControlNormal">#ffffffff</item>
    </style>
    <style name="Base.ThemeOverlay.AppCompat.ActionBar" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:colorControlNormal">#ffffffff</item>
    </style>
    <style name="Platform.AppCompat" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?buttonBarButtonStyle</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Display1" parent="@android:style/TextAppearance.Material.Display1" />
    <style name="Base.TextAppearance.AppCompat.Title" parent="@android:style/TextAppearance.Material.Title">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Medium" parent="@android:style/TextAppearance.Material.Medium">
        <item name="android:textColor">#ff1e1e1e</item>
    </style>
    <style name="Base.TextAppearance.AppCompat" parent="@android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Menu" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Menu">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:textColorPrimary">?android:colorAccent</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">#95000000</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?buttonBarButtonStyle</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Button" parent="@android:style/TextAppearance.Material.Button">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
