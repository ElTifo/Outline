<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Settings / Compose -->
    <style name="ColoredNavBarGeneralTheme" parent="@*com.google.android.gm:style/BaseGeneralTheme">
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:navigationBarColor">?android:colorBackground</item>
        <item name="android:statusBarColor">?android:colorBackground</item>
        <item name="android:colorBackground">@*android:color/background_material_dark</item>
        <item name="android:windowBackground">@*android:color/background_material_dark</item>
    </style>
    <style name="ColoredNavBarMailActivityTheme" parent="@*com.google.android.gm:style/BaseMailActivityTheme"/>
    <style name="ColoredNavBarGlifActivityTheme" parent="@*com.google.android.gm:style/SudThemeGlif.Light">
        <item name="android:navigationBarColor">?android:colorBackground</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">#0dFFFFFF</item>
    </style>
</resources>