<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Text -->
    <color name="textColor">#FFFFFF</color>                                                         <!--black -->

    <!-- Background -->
    <color name="mainBackgroundColor">@*android:color/background_material_dark</color>              <!-- Main           |horizontal empty space between the bar showing the current active out source and the eq levels area -->
    <color name="drawer_background_color">@*android:color/background_material_dark</color>          <!-- Drawer         | left drawer menu background color-->
    <color name="spinner_bg_color">@*android:color/background_floating_material_dark</color>        <!-- Spinner        | spinner box -->
    <color name="gallery_bg_color">@*android:color/background_material_dark</color>                 <!--                | horizontal bar where you can scroll the eq presets (after the eq and before the knobs -->
    <color name="knobs_area_background">@*android:color/background_material_dark</color>            <!--                | area around knobs also at right of the eq area -->
    <color name="eq_area_background">@*android:color/background_material_dark</color>               <!--                | eq levels area -->
    <color name="spinner_txt_area_color">@*android:color/background_material_dark</color>           <!-- Behind Spinner | "Reverb" txt area plus a small part at right of the spinner box -->
    <color name="current_out_source">@*android:color/background_material_dark</color>               <!-- Source Bar     | the bar with the current active out source icon and name -->
    <color name="drawer_header_color">@*com.android.musicfx:color/accentColor</color>               <!-- Drawer Header  | left drawer header background color-->

    <!-- Presets -->
    <color name="disabled_gallery_text">#616161</color> <!-- eq presets text color when musicfx is disabled -->
    <color name="highlight_gallery_text">#FFFFFF</color> <!-- eq presets text color when musicfx is on for the current selected preset -->
    <color name="lowlight_gallery_text">#99FFFFFF</color> <!-- eq presets text color when musicfx is on for other available presets -->
    <!-- Visualizer -->
    <color name="visualizer_highlight">#FFFFFF</color> <!-- equalizer active level bar top element  -->
</resources>